{
  "name": "@lievendoclo/gradle-aspect-sdm",
  "version": "0.1.0",
  "description": "my new project",
  "author": {
    "name": "lievendoclo",
    "email": "lieven@atomist.com"
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/lievendoclo/gradle-aspect-sdm#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/lievendoclo/gradle-aspect-sdm.git"
  },
  "bugs": {
    "url": "https://github.com/lievendoclo/gradle-aspect-sdm/issues"
  },
  "keywords": [
    "atomist",
    "automation",
    "sdm",
    "aspect"
  ],
  "main": "./index.js",
  "types": "./index.d.ts",
  "dependencies": {
    "@atomist/automation-client": "^1.7.0",
    "@atomist/sdm": "^1.7.0",
    "@atomist/sdm-core": "^1.7.0",
    "@atomist/sdm-pack-aspect": "^1.0.0-master.20190930134724",
    "@atomist/sdm-pack-fingerprint": "5.0.1-master.20190915182018",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@atomist/sdm-local": "^1.2.1",
    "@types/mocha": "^5.2.7",
    "@types/power-assert": "^1.5.0",
    "espower-typescript": "^9.0.2",
    "mocha": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.1",
    "rimraf": "^3.0.0",
    "supervisor": "^0.12.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "autotest": "supervisor --watch index.ts,lib,test --extensions ts --no-restart-on exit --quiet --exec npm -- test",
    "build": "run-s compile test lint doc",
    "clean": "run-p clean:compile clean:test clean:doc clean:run",
    "clean:compile": "rimraf git-info.json \"index.{d.ts,js}{,.map}\" \"{lib,test}/**/*.{d.ts,js}{,.map}\" lib/typings/types.ts",
    "clean:dist": "run-s clean clean:npm",
    "clean:doc": "rimraf doc",
    "clean:npm": "rimraf node_modules",
    "clean:run": "rimraf *-v8.log profile.txt log",
    "clean:test": "rimraf .nyc_output coverage",
    "compile": "run-s git:info gql:gen compile:ts",
    "compile:ts": "tsc --project .",
    "doc": "typedoc --mode modules --excludeExternals --ignoreCompilerErrors --exclude \"**/*.d.ts\" --out doc index.ts lib",
    "git:info": "atm-git-info",
    "gql:gen": "atm-gql-gen",
    "lint": "tslint --config tslint.json --format verbose --project .",
    "lint:fix": "npm run lint -- --fix",
    "start": "atm-start",
    "test": "mocha --require espower-typescript/guess \"test/**/*.test.ts\"",
    "test:one": "mocha --require espower-typescript/guess \"test/**/${TEST:-*.test.ts}\"",
    "typedoc": "npm run doc"
  },
  "engines": {
    "node": ">=8.2.0",
    "npm": ">=5.0.0"
  }
}
